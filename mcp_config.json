{
  "mcpServers": 
    {
        "context7": {
        "command": "npx",
        "args": [
            "-y",
            "@upstash/context7-mcp"
        ],
        "env": {
            "DEFAULT_MINIMUM_TOKENS": ""
        },
        "alwaysAllow": [
            "resolve-library-id",
            "get-library-docs"
        ]
        },
        "time": {
        "command": "uvx",
        "args": [
            "mcp-server-time"
        ],
        "alwaysAllow": [
            "get_current_time",
            "convert_time"
        ]
        },
        "exa": {
        "command": "npx",
        "args": [
            "-y",
            "@smithery/cli@latest",
            "run",
            "exa",
            "--key",
            "5476d42e-7554-4e5b-9099-c60c5250ba68",
            "--profile",
            "agreeable-cardinal-lr8729"
        ]
        }
       
    }
}